{
  "name": "generator-py",
  "version": "1.0.0",
  "description": "Yeoman generator to create a python project with test/coverage ready",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_DEBUG=generator-py nyc ava --timeout=10s --verbose",
    "test:dev": "NODE_DEBUG=generator-py nyc ava --timeout=10s --verbose && npm run report:dev",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "posttest": "npm run report",
    "report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "report:dev": "nyc report --reporter=html && npm run report:open",
    "report:open": "open coverage/index.html"
  },
  "files": [
    "generators"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/generator-py.git"
  },
  "keywords": [
    "generator-py",
    "yeoman",
    "yeoman-generator",
    "python",
    "pytest",
    "coverage"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false,
    "files": [
      "test/*.test.js"
    ]
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/generator-py/issues"
  },
  "devDependencies": {
    "@ostai/eslint-config": "^3.5.0",
    "ava": "^3.3.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "nyc": "^15.0.0"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "common-generator-prompts": "^2.0.0",
    "mustache": "^4.0.0",
    "scaffold-generator": "^3.0.4",
    "yeoman-generator": "^4.5.0",
    "yosay": "^2.0.2"
  }
}
